os: = import("os", "core")
strings: = import("strings", "core")

buf: byte[8]

for value in i64(0)..10
  do strings.println(strings.itoa(value, buf[]))

foo: vec4 = { 2, 4, 6, 8 }
for value in foo
  do strings.println(strings.itoa(i64(value), buf[]))

strings.println(strings.itoa(vec4.size, buf[]))
strings.println(vec4.name)

ints: i64[10]@os.heap
ints[3] = 42
strings.println(strings.itoa(ints[3], buf[]))
