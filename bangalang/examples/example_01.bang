increment_fourth_element := proc(arr: ^i32[8]) -> i32[8]
{
    arr[3] = arr[3] + 1
    return arr^
}

// Raises num to the power of exp
pow := proc(num: i32, exp: i64) -> i32
{
    result := num

    for (exp > 1)
    {
        result = result * num
        exp = exp - 1
    }

    return result
}
